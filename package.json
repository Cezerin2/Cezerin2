{
  "name": "cezerin2-mono",
  "version": "0.33.0",
  "description": "Cezerin is React and Node.js based eCommerce platform. https://cezerin.org",
  "keywords": [
    "cezerin",
    "ecommerce",
    "shopping cart",
    "shop",
    "cms"
  ],
  "homepage": "https://cezerin.org",
  "bugs": "https://github.com/Cezerin2/cezerin/issues",
  "license": "Unlicense",
  "author": "Restmount, Himadu, Community",
  "main": "src/api/server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cezerin2/cezerin"
  },
  "scripts": {
    "setup": "node -r esm src/api/server/setup.js",
    "compile": "babel src/store -d dist/store --presets @babel/react --plugins transform-class-properties --quiet",
    "compile:watch": "concurrently -n storeWatcher,themeWatcher \"babel src/store -d dist/store --presets react --plugins transform-class-properties --watch\" \"babel theme/src -d theme/dist --presets @babel/react --plugins transform-class-properties --watch\"",
    "webpack:admin": "webpack --config webpack.config.admin.js --mode production",
    "webpack:store": "webpack --config webpack.config.store.js --mode production",
    "webpack:admin:watch": "webpack --config webpack.config.admin.js --mode development --watch",
    "webpack:store:watch": "webpack --config webpack.config.store.js --mode development --watch",
    "theme:install": "./scripts/theme-install.sh",
    "theme:export": "./scripts/theme-export.sh",
    "theme:build": "babel theme/src -d theme/dist --presets @babel/react --plugins transform-class-properties --quiet && npm run webpack:store",
    "build": "npm run compile && npm run theme:build && npm run webpack:admin",
    "build:watch": "concurrently -n compile,webpackStore,webpackAdmin \"npm run compile:watch\" \"npm run webpack:store:watch\" \"npm run webpack:admin:watch\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write \"src/**/*.js\" \"theme/**/*.js\"",
    "precommit": "lint-staged",
    "start-api": "node -r esm src/api/server/index.js",
    "start-store": "node -r esm dist/store/server/index.js",
    "start": "concurrently npm:start-*",
    "quickstart": "yarn && yarn compile && yarn build && yarn start",
    "quickstartnpm": "npm i && npm run compile && npm run build && npm run start"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.1",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cezerin2-client": "^0.38.0",
    "chart.js": "^2.9.3",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-jwt": "^5.3.3",
    "formidable": "^1.2.2",
    "fs": "0.0.1-security",
    "fs-extra": "^9.0.0",
    "google-spreadsheet": "^3.0.11",
    "handlebars": "^4.7.6",
    "helmet": "^3.22.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^10.2.7",
    "lodash": "^4.17.15",
    "lru-cache": "^5.1.1",
    "lscache": "^1.3.0",
    "material-ui": "^0.20.2",
    "mini-css-extract-plugin": "^0.9.0",
    "moment": "^2.26.0",
    "mongodb": "^3.5.8",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.14.1",
    "nodemailer": "^6.4.8",
    "nodemailer-smtp-transport": "^2.7.4",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-helmet": "^6.0.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scroll": "^1.7.16",
    "react-sortable-hoc": "^1.11.0",
    "react-stripe-elements": "^6.1.2",
    "react-tagsinput": "^3.19.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.6",
    "redux-form-material-ui": "^4.3.4",
    "redux-thunk": "^2.3.0",
    "request-promise": "^4.2.5",
    "response-time": "^2.3.2",
    "sass-loader": "^8.0.2",
    "sitemap": "^6.1.5",
    "slug": "^3.2.0",
    "stripe": "^8.56.0",
    "theme": "./theme",
    "ua-parser-js": "^0.7.21",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "winston": "^3.2.1",
    "workbox-webpack-plugin": "^5.1.3",
    "ws": "7.3.0"
  },
  "devDependencies": {
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "prettier": "^2.0.5",
    "typescript": "^3.9.3"
  },
  "engines": {
    "node": ">8.0.0",
    "npm": ">5.0.0"
  }
}
