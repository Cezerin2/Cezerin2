{
  "name": "cezerin",
  "version": "0.33.0",
  "description": "Cezerin is React and Node.js based eCommerce platform. https://cezerin.org",
  "keywords": [
    "cezerin",
    "cezerin2",
    "ecommerce",
    "shopping cart",
    "shop",
    "cms"
  ],
  "homepage": "https://cezerin.org",
  "bugs": "https://github.com/Cezerin2/cezerin2/issues",
  "license": "MIT",
  "author": "Restmount <jsonmount@gmail.com> (https://github.com/restmount)",
  "main": "src/api/server/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cezerin2/cezerin2"
  },
  "scripts": {
    "setup": "ts-node src/api/server/setup.ts",
    "compile": "babel src/store -d dist/store --quiet",
    "compile:watch": "concurrently -n storeWatcher,themeWatcher \"babel src/store -d dist/store --watch\" \"babel theme/src -d theme/dist --extensions \".tsx\" --watch\"",
    "webpack:admin": "webpack --config webpack.config.admin.ts --mode production",
    "webpack:store": "webpack --config webpack.config.store.ts --mode production",
    "webpack:admin:watch": "webpack --config webpack.config.admin.ts --mode development --watch",
    "webpack:store:watch": "webpack --config webpack.config.store.ts --mode development --watch",
    "theme:install": "./scripts/theme-install.sh",
    "theme:export": "./scripts/theme-export.sh",
    "theme:build": "npm run theme:compile && npm run webpack:store",
    "theme:compile": "babel theme/src -d theme/dist --extensions \".tsx\" --quiet",
    "build": "npm run compile && npm run theme:build && npm run webpack:admin",
    "build:watch": "concurrently -n compile,webpackStore,webpackAdmin \"npm run compile:watch\" \"npm run webpack:store:watch\" \"npm run webpack:admin:watch\"",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ignore-path .gitignore .",
    "format": "prettier --write .",
    "start:api": "ts-node src/api/server/index.ts",
    "start:store": "node dist/store/server/index.js",
    "start": "concurrently npm:start:*",
    "watch:api": "tsnd --respawn src/api/server/index.ts"
  },
